package com.barcap.simulator.oms;

/**
 *
 * @author marco
 */
public enum OrderStatus {
    NEW("0"),
    PARTIAL_FILL("1"),
    FILL("2"),
    DONE("3"),
    CANCELED("4"),
    REPLACED("5"),
    PENDING_CANCEL("6"),
    STOPPED("7"),
    REJECTED("8"),
    SUSPENDED("9"),
    PENDING_NEW("A"),
    CALCULATED("B"),
    EXPIRED("C"),
    ACCEPTED_FOR_BIDDING("D"),
    PENDING_REPLACE("E");


    public String content;

    OrderStatus(String content) {
        this.content = content;
    }

    public String value() {
        return content;
    }


}
